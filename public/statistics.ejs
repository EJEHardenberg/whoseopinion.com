<head>
<%- partial("_inc/styles") %>
</head>
<div id="statistics">
	<script type="text/javascript">
		window.dummyMapInfo = <%- JSON.stringify(dummydata.statistics) %>
	</script>
	<%- partial("_inc/scripts.ejs") %>
	<h2>Opinion Demographics</h2>
	<div id="map" style="width: 400px; height: 250px;"></div>
	<div>
		
		<ul><% 
			var ranks = ["red", "orange","olive","green","blue"]
			for(r in ranks){
				var color = ranks[r] %>
				<li style="background-color: <%= color %>"><span><%= labels[r] %></span><%
				} %>
		</ul>
		<p>
			Ever wonder where the people who think like you are?
		</p>
	</div>
</div>